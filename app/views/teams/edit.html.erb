<img src="/images/242x82.png"> </img>


<div class="signup_block">
  <%= form_for @team  do|f| %>
      <div class="section_block" id="section_block">
        <div class="login_content">
          <div class="login-head">
            <h1 style="color:darkred">Team Page</h1>
          </div>
        </div>
        <% if @team.errors.any? %>
            <div id="error_explanation">
              <h2>
                <%= pluralize(@team.errors.count, "error") %> prohibited
                this article from being saved:
              </h2>
              <ul>
                <% @team.errors.full_messages.each do |msg| %>
                    <li><%= msg %></li>
                <% end %>
              </ul>
            </div>
        <% end %>

        <div class="form-group">
          <%= f.label :team_name %>
          <%= f.select(:team_name, options_for_select([['CORE','CORE'],['DESIGN','DESIGN'],['IMPLEMENTATION','IMPLEMENTATION'],['INFRASTRUCTURE','INFRASTRUCTURE'],['PRODUCTION','PRODUCTION'],['ADMIN','ADMIN']])) %>
        </div>

        <div class="form-group">
          <%= f.label :team_head %>
          <%= f.text_field :team_head %>
        </div>



        <div class="form-group">
          <%= f.label :summary %>
          <%= f.text_field :summary %>
        </div>



        <div class="form-group">
          <%= f.submit %>
        </div>

  <% end %>
  <b><%= link_to 'Back', teams_path %></b>
  </div>
</div>