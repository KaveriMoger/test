<img src="/images/242x82.png"> </img>


<div class="signup_block">
  <div class="section_block" id="section_block">
    <div class="login_content">
      <div class="login-head">
        <h1 style="color:darkred">Client Page</h1>
      </div>
      <div class="form-group">
        <strong>Client-Name:</strong>
        <%= @client.c_name %>
      </div>

      <div class="form-group">
        <strong>Client-Email:</strong>
        <%= @client.c_email %>
      </div>


      <div class="form-group">
        <strong>Client-Website:</strong>
        <%= @client.c_website %>
      </div>

      <div class="form-group">
        <strong>Client-Country:</strong>
        <%= @client.c_country %>
      </div>





      <b><%= link_to 'Back', clients_path %></b>
    </div>
  </div>
</div>

